<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!-- 超凡万劫科技 - 基础 -->
  <ResearchProjectDef Abstract="True" Name="ExtraordinaryWanjietechBase">
    <techLevel>Spacer</techLevel>
    <tab>Wanjietech</tab>
    <requiredResearchBuilding>HiTechResearchBench</requiredResearchBuilding>
    <requiredResearchFacilities>
      <li>MultiAnalyzer</li>
    </requiredResearchFacilities>
  </ResearchProjectDef>

  <!-- 超凡万劫科技(蓝图) - 基础 -->
  <ResearchProjectDef Abstract="True" Name="ExtraordinaryWanjietechBase_Techprint">
    <techLevel>Spacer</techLevel>
    <tab>Wanjietech</tab>
    <requiredResearchBuilding>HiTechResearchBench</requiredResearchBuilding>
    <requiredResearchFacilities>
      <li>MultiAnalyzer</li>
    </requiredResearchFacilities>
    <techprintCount>1</techprintCount>
    <techprintCommonality>2</techprintCommonality>
    <techprintMarketValue>2000</techprintMarketValue>
    <heldByFactionCategoryTags>
      <li>Empire</li>
      <li>Outlander</li>
    </heldByFactionCategoryTags>
  </ResearchProjectDef>

  <!-- 超凡万劫科技 -->
  <ResearchProjectDef>
    <defName>ExtraordinaryWanjietech</defName>
    <label>Extraordinary Wanjietech</label>
    <description>You are about to reach the peak of Wanjietech, but you cannot complete many
      research projects independently.</description>
    <baseCost>4000</baseCost>
    <techLevel>Spacer</techLevel>
    <researchViewX>7</researchViewX>
    <researchViewY>3</researchViewY>
    <tab>Wanjietech</tab>
    <requiredResearchBuilding>HiTechResearchBench</requiredResearchBuilding>
    <requiredResearchFacilities>
      <li>MultiAnalyzer</li>
    </requiredResearchFacilities>
    <prerequisites>
      <li>AdvancedWanjietech</li>
    </prerequisites>
  </ResearchProjectDef>

  <!-- 超凡水栽培 -->
  <ResearchProjectDef ParentName="ExtraordinaryWanjietechBase_Techprint">
    <defName>WanjietechExtraordinaryHydroponicsBasin</defName>
    <label>Extraordinary Hydroponics Basin</label>
    <description>You find that you can use high-energy cores to control the energy input into
      plants, so that you can greatly improve the growth rate of plants. In addition, you also
      incidentally studied a kind of mini hydroponics, which only occupies one square.</description>
    <baseCost>2000</baseCost>
    <researchViewX>8</researchViewX>
    <researchViewY>6</researchViewY>
    <prerequisites>
      <li>WanjietechHighEnergyHydroponicsBasin</li>
      <li>ExtraordinaryWanjietech</li>
    </prerequisites>
  </ResearchProjectDef>

  <!-- 万劫超凡电池 -->
  <ResearchProjectDef ParentName="ExtraordinaryWanjietechBase_Techprint">
    <defName>WanjietechExtraordinaryBattery</defName>
    <label>Extraordinary Batteries</label>
    <description>Unlock the most advanced battery of Wanjietech.</description>
    <baseCost>2500</baseCost>
    <researchViewX>8</researchViewX>
    <researchViewY>6.5</researchViewY>
    <prerequisites>
      <li>WanjietechBatteries</li>
      <li>ExtraordinaryWanjietech</li>
    </prerequisites>
  </ResearchProjectDef>
</Defs>
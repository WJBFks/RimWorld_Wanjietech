<?xml version="1.0" encoding="utf-8"?>

<Defs>
  <!-- 高能水栽培 -->
  <ThingDef ParentName="BuildingBase">
    <defName>HighEnergyHydroponicsBasin</defName>
    <label>high-energy hydroponics basin</label>
    <description>The improved hydroponics injects electricity into the plant to help it grow rapidly, but because of its high energy, no other plant can grow in it except high-energy fruit.</description>
    <thingClass>Building_PlantGrower</thingClass>
    <graphicData>
      <texPath>Things/Building/Production/HighEnergyHydroponicsBasin</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(1,4)</drawSize>
      <damageData>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
    </graphicData>
    <castEdgeShadows>true</castEdgeShadows>
    <altitudeLayer>LowPlant</altitudeLayer>
    <defaultPlacingRot>East</defaultPlacingRot>
    <passability>PassThroughOnly</passability>
    <pathCost>30</pathCost>
    <constructEffect>ConstructMetal</constructEffect>
    <fillPercent>0.30</fillPercent>
    <canOverlapZones>false</canOverlapZones>
    <constructionSkillPrerequisite>4</constructionSkillPrerequisite>
    <statBases>
      <WorkToBuild>3000</WorkToBuild>
      <MaxHitPoints>200</MaxHitPoints>
      <Flammability>0.5</Flammability>
      <Cleanliness>-3</Cleanliness>
    </statBases>
    <size>(1,4)</size>
    <costList>
      <Steel>100</Steel>
      <HighEnergyChip>1</HighEnergyChip>
    </costList>
    <fertility>5</fertility>
    <building>
      <defaultPlantToGrow>Plant_HighEnergyFruit</defaultPlantToGrow>
      <sowTag>HighEnergyHydroponic</sowTag>
      <buildingTags>
        <li>Production</li>
      </buildingTags>
    </building>
    <tickerType>Rare</tickerType>
    <comps>
      <li Class="CompProperties_Forbiddable" />
      <li Class="CompProperties_Power">
        <compClass>CompPowerTrader</compClass>
        <basePowerConsumption>150</basePowerConsumption>
      </li>
      <li Class="CompProperties_Flickable" />
    </comps>
    <researchPrerequisites>
      <li>WanjietechHighEnergyHydroponicsBasin</li>
    </researchPrerequisites>
    <terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
    <designationCategory>Wanjietech</designationCategory>
  </ThingDef>


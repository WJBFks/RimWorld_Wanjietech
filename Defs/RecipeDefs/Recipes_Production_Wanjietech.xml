<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <!-- 制作高能胶囊外壳 -->
  <RecipeDef>
    <defName>Make_HighEnergyCapsuleShell</defName>
    <label>make high-energy capsule shell</label>
    <description>Make a high-energy capsule shell.</description>
    <jobString>Making high-energy capsule shell.</jobString>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workAmount>1500</workAmount>
    <unfinishedThingDef>UnfinishedHighEnergyCapsuleShell</unfinishedThingDef>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>8</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Steel</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <HighEnergyCapsuleShell>1</HighEnergyCapsuleShell>
    </products>
    <skillRequirements>
      <Crafting>4</Crafting>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
    <researchPrerequisite>ModernWanjietech</researchPrerequisite>
    <recipeUsers>
      <li>TableMachining</li>
      <li>ElectricSmithy</li>
      <li>FueledSmithy</li>
    </recipeUsers>
  </RecipeDef>

